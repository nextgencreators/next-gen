{% embed 'partials/base.html.twig' %}
{% block content %}
<div class="indiv-img-cover-wrapper">
   {% set image = page.media.images|first %}
   {% if image != null %}
   {{ image.cropZoom(960,320).html('','','indiv-img-cover') }}
   {% endif %}
</div>
<!-- {#<div class="event-container">
   <section class="event-listing">
      {% if config.plugins.breadcrumbs.enabled %}
      {% include 'partials/breadcrumbs.html.twig' %}
      {% endif %}
      {% include 'partials/event_item.html.twig' with {'event':page.parent, 'truncate':false, 'single':true} %}
   </section>
   {% include 'partials/events_sidebar.html.twig' with {'page':page.parent} %}
</div>
#} -->
<br>
<br>
<div class="section no-pad-bot" id="index-banner">
   <div class="container">
   
        
         <h4 class="header white-text left-align" id="event-title">{{ page.title }}</h4>
         <div class="time-info-right right-align">
         <span class="day day-info-right white-text">{{ config.plugins.events.date_format.translate ? 'MONTHS_OF_THE_YEAR'|ta(page.header.event.start|date('n') - 1) : page.header.event.start|date('M') }} {{ page.header.event.start|date("d") }}</span>
         <br>
         <span class="time time-info-bottom">
         {{ page.header.event.start|date('g:ia') }}&ndash;{{ page.header.event.end|date('g:ia') }}
         </span>
       </div>
     
         <!-- {#<span class="month">{{ config.plugins.events.date_format.translate ? 'MONTHS_OF_THE_YEAR'|ta(page.header.event.start|date('n') - 1) : page.header.event.start|date('M') }} {{ page.header.event.start|date("d") }},</span>
         <span class="year"> {{ page.header.event.start|date("Y") }}</span>
         <br>
         <span class="time">
         {{ page.header.event.start|date('g:ia') }}&ndash;{{ page.header.event.end|date('g:ia') }}
         </span>#} -->
         <div class="section white-text source-sans-pro">
            {{ page.content }}
         </div>
         <div class="center lato" style="padding-top: 3em;">
               <a class="s-green waves-effect waves-light btn">volunteer</a>
               <a class="s-green waves-effect waves-light btn">donate</a>
               <a class="s-green waves-effect waves-light btn">sponsor</a>
            </div>
      </div>

</div>
{% endblock %}
{% endembed %}